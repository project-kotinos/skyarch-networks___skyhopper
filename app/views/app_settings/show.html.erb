<!-- 一番上に追加 -->
<% provide(:title, I18n.t("app_settings.title.setup")) %>
<h1 class="page-header"><%= t 'app_settings.title.show' %></h1>

<div id="indexElement" class="panel panel-primary" v-cloak>
  <div class="alert alert-dismissible alert-warning" v-if="!isAllowedBrowser">
    <button type="button" class="close" data-dismiss="alert">&times;</button>
    <h4><%= I18n.t("app_settings.warning") %></h4>
    <p><%= raw I18n.t("app_settings.msg.warning_msg") %></p>
  </div>
  <div class="panel-heading">
    <%= I18n.t("app_settings.title.setup") %>

  </div>
  <div class="panel-body">
    <%= render partial: 'panel_content_system' %>
    <%= render partial: 'panel_content_chef' %>
  </div>

  <div class="panel-footer">
    <div class="btn-toolbar">
      <%= link_to t('app_settings.btn.done'), root_path, class: 'btn btn-primary disabled', id: 'done-appsetting', disabled: "disabled" %>
    </div>
  </div>
</div>
